{
    sprite_index = spr_Player_Right;
    obj_Player.x += movement_Speed;
    image_speed = .8
    show_debug_message("move right");
    last_Direction = sprite_index;
    //global.Dir = 1
}
else
{
    // do nothing
    sprite_index = last_Direction;
    image_speed = 0;
